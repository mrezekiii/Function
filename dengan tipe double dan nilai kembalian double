#include <iostream>
using namespace std;
//TUGAS PRAKTIKUM DIRUMAH (Function dengan tipe double dan nilai kembalian double) 21/04/2020 MUHAMMAD REZEKI
//Function dengan tipe double dan nilai kembalian double
void function ()
{
	cout << "Function dengan tipe double dan nilai kembalian double" << endl;
	
}

double Pembagian(double nilai1, double nilai2)
{
	return  nilai1 / nilai2;
}

int main ()
{
	function ();
	cout << "Hasil Pembagian dari (100,50) : " <<endl << Pembagian(100,50);
	return 0;
}
